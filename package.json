{
  "name": "typescript-express-api",
  "version": "0.0.1",
  "description": "TypeScript based express.js API starter.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "date +\"%T\" && echo \"Cleaning...\" && rm -rf ./js && echo \"Compiling...\" && tsc && echo \"Done!\" && date +\"%T\"",
    "start": "node ./js/src/index.js",
    "dev": "npx nodemon ./ts/src/index.ts",
    "compile": "npm run build && npm run start",
    "run-on-ts-node": "ts-node ./ts/src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GokselKUCUKSAHIN/typescript-express-api.git"
  },
  "keywords": [
    "api",
    "typescript",
    "express.js",
    "starter"
  ],
  "author": "GokselKUCUKSAHIN <gokselkucuksahin@gmail.com> (https://github.com/GokselKUCUKSAHIN)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GokselKUCUKSAHIN/typescript-express-api/issues"
  },
  "homepage": "https://github.com/GokselKUCUKSAHIN/typescript-express-api#readme",
  "dependencies": {
    "@jellybeanci/is-empty": "^0.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "ts-node": "^10.9.1",
    "nodemon": "^2.0.19"
  }
}